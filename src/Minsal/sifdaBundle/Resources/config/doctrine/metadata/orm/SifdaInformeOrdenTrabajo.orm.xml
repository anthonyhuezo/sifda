      <index name="realiza_fk" columns="id_dependencia_establecimiento"/>
      <index name="requiere_fk" columns="id_subactividad"/>
      <index name="se_generan_fk" columns="id_etapa"/>
      <index name="IDX_24D08FE9890253C7" columns="id_empleado"/>
